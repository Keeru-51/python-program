print("Hello World");



#hello world with tab
print("Hello World\t Hello World");



#Hello world with new line
print("Hello world \n Hello world ");


#Integer Input
a=int(input())
print(a)



#Float Input
P=float(input())
print('%2f'%P)


#Character Input
a=input()
print(a)


#String Input
a=str(input())
print(a)


#Ascii Code1
a=input()
print(ord(a))


#Ascii Code2
a=int(input())
print(chr(a))
